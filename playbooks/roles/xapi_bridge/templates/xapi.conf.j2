[program:xapi]

command={{ xapi_bridge_venv_bin }}/python xapi_bridge {{ COMMON_LOG_DIR }}/tracking/tracking.log
user={{ xapi_bridge_user }}
directory={{ xapi_bridge_code_dir }}
stdout_logfile={{ supervisor_log_dir }}/%(program_name)s-stdout.log
stderr_logfile={{ supervisor_log_dir }}/%(program_name)s-stderr.log
killasgroup=true
stopasgroup=true
